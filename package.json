{
  "name": "obfuscation-aware-sanitizer",
  "version": "1.0.0",
  "description": "API for sanitizing data in agentic AI systems",
  "main": "src/app.js",
  "scripts": {
    "format": "prettier --write src",
    "format:check": "prettier --check src",
    "lint": "eslint src",
    "monitor:health": "node scripts/agent-message-health-monitor.js",
    "start": "node src/app.js",
    "test": "jest",
    "test:coverage": "jest --coverage && npm run test:coverage:verify && npm run test:performance",
    "test:coverage:verify": "node scripts/verify-coverage.js",
    "test:edge-cases": "node scripts/run-edge-case-tests.js",
    "test:integration": "jest src/tests/integration",
    "test:load": "node scripts/load-test-agent-messages.js",
    "test:performance": "node scripts/measure-performance.js",
    "test:regression": "node scripts/run-regression-tests.js",
    "test:unit": "jest src/tests/unit"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.js",
      "!src/app.js"
    ],
    "coverageDirectory": "coverage",
    "coverageReporters": [
      "text",
      "lcov",
      "html"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/jest.setup.js"
    ],
    "testEnvironment": "node",
    "transformIgnorePatterns": [
      "node_modules/(?!(pdfjs-dist|jsdom|@langchain|langchain|p-retry|@langchain/core)/)"
    ]
  },
  "dependencies": {
    "@google/generative-ai": "^0.24.1",
    "@langchain/core": "^1.0.5",
    "@langchain/google-genai": "^2.0.0",
    "@langchain/openai": "^1.1.1",
    "better-queue": "^3.8.12",
    "csv-writer": "^1.6.0",
    "dotenv": "^17.2.3",
    "express": "^4.18.2",
    "express-rate-limit": "^8.1.0",
    "joi": "^17.11.0",
    "langchain": "^1.0.4",
    "multer": "^2.0.2",
    "openai": "^6.9.0",
    "parquetjs-lite": "^0.8.7",
    "pdf-parse": "^1.1.4",
    "pdfjs-dist": "^5.4.394",
    "pdfkit": "^0.17.2",
    "sqlite3": "^5.1.7",
    "supertest": "^6.3.3",
    "unorm": "^1.6.0",
    "winston": "^3.11.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.38.0",
    "artillery": "^2.0.27",
    "better-queue-sql": "^1.0.6",
    "bleach": "^0.3.0",
    "dompurify": "^3.3.0",
    "eslint": "^9.38.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-n": "^17.23.1",
    "eslint-plugin-unicorn": "^61.0.2",
    "eslint-plugin-yml": "^1.19.0",
    "jest": "^29.7.0",
    "jsdom": "^27.2.0",
    "prettier": "^3.6.2",
    "prettier-plugin-packagejson": "^2.5.19",
    "proxyquire": "^2.1.3",
    "rewire": "^9.0.1",
    "sanitize-html": "^2.17.0",
    "sinon": "^21.0.0"
  },
  "engines": {
    "node": ">=20.11.0"
  }
}
