{
  "name": "obfuscation-aware-sanitizer",
  "version": "1.0.0",
  "description": "API for sanitizing data in agentic AI systems",
  "main": "src/app.js",
  "scripts": {
    "format": "prettier --write src",
    "format:check": "prettier --check src",
    "lint": "eslint src",
    "start": "node src/app.js",
    "test": "jest",
    "test:integration": "jest src/tests/integration",
    "test:unit": "jest src/tests/unit"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.js",
      "!src/app.js"
    ],
    "coverageDirectory": "coverage",
    "coverageReporters": [
      "text",
      "lcov",
      "html"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/jest.setup.js"
    ],
    "testEnvironment": "node"
  },
  "dependencies": {
    "better-queue": "^3.8.12",
    "csv-writer": "^1.6.0",
    "dotenv": "^17.2.3",
    "express": "^4.18.2",
    "express-rate-limit": "^8.1.0",
    "joi": "^17.11.0",
    "multer": "^2.0.2",
    "parquetjs-lite": "^0.8.7",
    "pdf-lib": "^1.17.1",
    "pdf-parse": "^2.4.5",
    "pdfkit": "^0.17.2",
    "sqlite3": "^5.1.7",
    "supertest": "^6.3.3",
    "unorm": "^1.6.0",
    "winston": "^3.11.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.38.0",
    "better-queue-sql": "^1.0.6",
    "eslint": "^9.38.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-n": "^17.23.1",
    "eslint-plugin-unicorn": "^61.0.2",
    "eslint-plugin-yml": "^1.19.0",
    "jest": "^29.7.0",
    "jsdom": "^27.0.1",
    "prettier": "^3.6.2",
    "prettier-plugin-packagejson": "^2.5.19",
    "proxyquire": "^2.1.3",
    "rewire": "^9.0.1",
    "sinon": "^21.0.0"
  },
  "engines": {
    "node": ">=20.11.0"
  }
}
